# Create a tuple
fruits = ("apple", "banana", "citrus", "durian")

( "a" , 1 , 2.3 , "b"   )


# Access elements by index
third_fruit = fruits[2]
fourth_fruit = fruits[3]

# Iterate through the tuple
print("Fruits available:")
for fruit in fruits:
    print(fruit)

# Check if an element is in the tuple
contains_banana = 'banana' in fruits

# Find the length of the tuple
length_fruits = len(fruits)

# Concatenate two tuples
more_fruits = ("pineapple", "dragonfruit")
extra_fruits = fruits + more_fruits

# Nested tuple
nested_tuple = ("red", ("blue", "purple"))

# Print the results
print("Third fruit:", third_fruit)
print("Fourth fruit:", fourth_fruit)
print("Does it contain 'banana'? ", contains_banana)
print("Number of fruits:", length_fruits)
print("All fruits:", extra_fruits)
print("Nested tuple:", nested_tuple)

